#define sum(...)          \
int arr[]={__VA_ARGS__};  \
int tong=0;               \
int i = 0;                \
while (arr[i] !=0         \
{                         \
    tong += arr[i];       \
    i++                   \
}                         \
printf("tong: %d/n", tong);
{
sum (1,2,3,4,5);
return 0
}
